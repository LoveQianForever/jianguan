.css-rad-menubody {
	/*菜单页面背景*/
	background-color:#dfdfff;
	maigin: 0;
	margin-left: 0px;
}
div.sdmenu {
	width: 130px;
	font-family: "宋体";
	font-weight: bold;
	font-size: 12px;
	padding-bottom: 10px;
	background: url(images/bottom.gif) no-repeat  right bottom;
	color: #004a7e;
	filter: DropShadow(Color=#FFFFFF, OffX=1, OffY=1, Positive=1,1);
}

div.sdmenu table {
	border-collapse:collapse;
	border-spacing:0;
	
	
}

div.sdmenu tr,td {
	padding: 0;
}



div.sdmenu div {
	background: url(images/blue-botton1.jpg) no-repeat;
	overflow: hidden;
}
div.sdmenu div:first-child {
	background: url(images/blue-botton1.jpg) no-repeat;
}
div.sdmenu div.collapsed {
	height: 25px;
}
div.sdmenu div span {
	display: block;
	padding: 5px 25px;
	font-weight: bold;
	color: #white;
	background: url(images/class_exp_blue.gif) no-repeat 10px center;
	cursor: pointer;
	border-bottom: 1px solid #ddd;
}
div.sdmenu div.collapsed span {
	background-image: url(images/class_col_blue.gif);
}
.p {
	padding: 5px 10px;
